[user]
	email = chris.lasher@gmail.com
	name = Chris Lasher
[color]
	ui = auto
[core]
	excludesfile = ~/.gitignore_global
[github]
	user = gotgenes
[alias]
	ci = commit
	; Superb log aliases from
	; http://www.jukie.net/bart/blog/pimping-out-git-log
	log1 = log --pretty=oneline --abbrev-commit --decorate
	glog = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative --date-order
[push]
	default = matching
